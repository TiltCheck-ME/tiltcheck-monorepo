{
  "name": "@tiltcheck/session-store",
  "version": "0.1.0",
  "description": "In-memory session store with optional Redis backend",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "typescript": "^5.3.0",
    "vitest": "^2.0.0"
  },
  "optionalDependencies": {
