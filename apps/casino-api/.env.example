# TiltCheck Casino Data API - Environment Variables
# Copy to .env and fill in values before running

# Server port (default: 6002)
CASINO_API_PORT=6002

# Internal API key to authenticate requests to this service
# Generate with: openssl rand -hex 32
CASINO_API_KEY=change_this_to_a_random_key

# TiltCheck platform API URL and key (for cross-service calls)
TILTCHECK_API_URL=https://tiltcheck.me/api
TILTCHECK_API_KEY=change_this_to_a_random_key
